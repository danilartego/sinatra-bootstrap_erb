<table class="table table-striped">
  <thead>
    <tr>
      <th>Имя</th>
      <th>Телефон</th>
      <th>Дата</th>
      <th>Барбер</th>
      <th>Цвет</th>
    </tr>
  </thead>
  <tbody>
    <% @db_users.each do |user| %>
    <tr>
      <td><%= user["username"] %></td>
      <td><%= user["phone"] %></td>
      <td><%= user["datestamp"] %></td>
      <td><%= user["barber"] %></td>
      <td style="background: <%= user["color"] %>"><%= user["color"] %></td>
    </tr>
    <% end %>
  </tbody>
</table>

